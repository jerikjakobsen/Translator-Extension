{
    "manifest_version": 3,
    "name": "Translator",
    "description": "Translating videos on the fly.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["modules/IntermediateAudioProcessor.js"],
            "matches": ["<all_urls>"]
        }
    ]
}